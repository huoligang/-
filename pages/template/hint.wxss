.tem-reportBox{
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  position: fixed;
  z-index: 666;
  top: 50%;
  margin-top: -180rpx;
}
.tem-report{
  width: 580rpx;
  height: 360rpx;
  border-radius: 10px;
  background: linear-gradient(#9a9a9a, #fcfcfc);
  display: flex;
  align-items: center;
  /* justify-content: center; */
  flex-direction: column;
  position: relative;
  z-index: 666;
}
.tem-img1{
  width: 155rpx;
  height: 37rpx;
  display: block;
  position: relative;
  z-index: 666;
  margin-top: 60rpx;
}
.tem-text1{
  width: 420rpx;
  font-size: 28rpx;
  color: #333333;
  line-height: 33rpx;
  margin-top: 55rpx;
}
.tem-btn1{
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  font-size: 28rpx;
  color: #333333;
  box-sizing: border-box;
  border-top: 1px solid #2e2f33;
  position: absolute;
  bottom:0;
  left: 0; 
}
.tem-pop_up{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  z-index: 666;
}